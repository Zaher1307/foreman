service_ping:
  cmd: ping -c 20 google.com 
  checks:
    cmd: ping -c 1 google.com
  deps: 
      - service_redis

service_sleep:
  cmd: sleep infinity
  checks:
    cmd: sleep 1
  deps: 
      - service_ping

service_redis:
  cmd: redis-server --port 6010
  run_once: true
  checks:
    cmd: redis-cli -p 6010 ping
